{
    "compilerOptions": {
        "declaration": true,
        "outDir": "./dist/",
        "baseUrl": "./src/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "paths": {
            "src/*": ["src/*"]
        }
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ],
    "awesomeTypescriptLoaderOptions": {}
}